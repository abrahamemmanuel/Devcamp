{
  "name": "Devcamp-io",
  "version": "1.0.0",
  "description": "Developer coding platform",
  "main": "server.js",
  "scripts": {
    "test": "cross-env NODE_ENV=test nyc mocha ./src/test/**/**.test.js",
    "cover": "nyc report --reporter=text-lcov | coveralls",
    "dev": "nodemon --exec babel-node ./src/server.js",
    "lint": "eslint **/*.js"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "ejs": "^2.6.1",
    "express": "^4.16.4"
  },
  "devDependencies": {
    "@babel/cli": "^7.2.0",
    "@babel/core": "^7.2.2",
    "@babel/node": "^7.2.2",
    "@babel/preset-env": "^7.2.0",
    "@babel/register": "^7.0.0",
    "babel-plugin-istanbul": "^5.1.0",
    "chai": "^4.2.0",
    "coveralls": "^3.0.2",
    "cross-env": "^5.2.0",
    "eslint": "^5.9.0",
    "istanbul": "^0.4.5",
    "istanbul-coveralls": "^1.0.3",
    "mocha": "^5.2.0",
    "mocha-lcov-reporter": "^1.3.0",
    "nodemon": "^1.18.9",
    "nyc": "^13.1.0",
    "supertest": "^3.3.0"
  },
  "nodemonConfig": {
    "restartable": "rs",
    "delay": "2500"
  },
  "nyc": {
    "check-coverage": true,
    "per-file": true,
    "lines": 50,
    "statements": 50,
    "functions": 50,
    "branches": 50,
    "require": [
      "@babel/register"
    ],
    "reporter": [
      "text-summary"
    ],
    "sourceMap": false,
    "instrument": true
  }
}
